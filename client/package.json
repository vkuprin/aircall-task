{
  "name": "aircall-task",
  "private": true,
  "version": "0.1.0",
  "author": "Vitaly Kuprin",
  "profile": "https://www.linkedin.com/in/vitaly-kuprin/",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "yarn eslint",
    "lint:fix": "eslint --fix src",
    "lint:all": "eslint src",
    "rebuild": "rm -rf yarn.lock && rimraf node_modules && yarn install",
    "typescript": "tsc -p ./tsconfig.json --pretty --noEmit",
    "test:unit": "jest",
    "test:end2end": "yarn playwright test",
    "test:report": "npx playwright show-report"
  },
  "engines": {
    "node": "18.0.0",
    "yarn": "3.2.0"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "yarn lint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@3.2.0",
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@types/uuid": "^8.3.4",
    "antd": "^4.24.0",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.4.3",
    "react-use": "^17.4.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.27.1",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-react": "^2.2.0",
    "eslint": "8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "lint-staged": "^13.0.3",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.2"
  }
}
